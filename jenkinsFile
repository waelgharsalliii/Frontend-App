pipeline {
    agent any

    stages {
        stage('Checkout Git') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '*/main']],
                userRemoteConfigs: [[url: 'git@github.com:waelgharsalliii/backend_App.git',
                                    credentialsId: 'Jenkins-git-ssh']]])                
            }
        }